class Sensors(Base):
    __tablename__ = "Sensors"

    id: Mapped[str] = mapped_column(String(32), primary_key=True)
    RiverId: Mapped[int] = mapped_column(Integer, ForeignKey("Rivers.RiverId"), nullable=True)
